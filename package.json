{
  "name": "team_manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "npx prisma generate && next build",
    "start": "npx prisma migrate deploy && node server.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 3 --fix",
    "test": "jest --passWithNoTests",
